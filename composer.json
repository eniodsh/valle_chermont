{
	"name": "part/cms-cakephp",
	"description": "Projeto CMS CakePHP, criado por Part Comunicação",
	"keywords": ["part","cms","cakephp"],
	"homepage": "http://part.com.br",
	"authors": [
		{
			"name": "Matheus Frade",
			"homepage": "https://github.com/matheusfrade/"
