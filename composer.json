		{
			"name": "Ricardo Vigatti",
			"homepage": "https://github.com/ricardovigatti/"
		},
		{
			"name": "Thiago Carneiro",
			"homepage": "https://github.com/thiagopart/"
		}
	],
	"support": {
		"issues": "https://github.com/ricardovigatti/cms-part-cakephp/issues",
